{
  "openapi": "3.0.0",
  "info": {
    "title": "Binar Car API",
    "version": "1.0.0",
    "description": "API documentation for Binar Car rental service"
  },
  "servers": [
    {
      "url": "http://localhost:3000/api/v1",
      "description": "Development server"
    }
  ],
  "components": {
    "securitySchemes": {
      "bearerAuth": {
        "type": "http",
        "scheme": "bearer",
        "bearerFormat": "JWT"
      }
    },
    "schemas": {
      "LoginRequest": {
        "type": "object",
        "required": ["email", "password"],
        "properties": {
          "email": {
            "type": "string",
            "format": "email",
            "example": "superadmin@binar.com"
          },
          "password": {
            "type": "string",
            "example": "superadmin123"
          }
        }
      },
      "RegisterRequest": {
        "type": "object",
        "required": ["email", "password", "name"],
        "properties": {
          "email": {
            "type": "string",
            "format": "email",
            "example": "zainal@gmail.com"
          },
          "password": {
            "type": "string",
            "example": "password123"
          },
          "name": {
            "type": "string",
            "example": "Zainal"
          }
        }
      },
      "CarRequest": {
        "type": "object",
        "required": ["name", "type", "price", "available", "image"],
        "properties": {
          "name": {
            "type": "string",
            "example": "Toyota Camry"
          },
          "type": {
            "type": "string",
            "example": "Sedan"
          },
          "price": {
            "type": "number",
            "example": 35000
          },
          "available": {
            "type": "boolean",
            "example": true
          },
          "image": {
            "type": "string",
            "format": "uri",
            "example": "https://example.com/car-image.jpg"
          }
        }
      }
    }
  },
  "paths": {
    "/users/login": {
      "post": {
        "tags": ["Users"],
        "summary": "Login user",
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/LoginRequest"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Login successful",
            "content": {
              "application/json": {
                "example": {
                  "status": "Success",
                  "message": "Login successful",
                  "isSuccess": true,
                  "data": {
                    "token": "eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9...",
                    "user": {
                      "id": 1,
                      "email": "superadmin@binar.com",
                      "role": "superadmin"
                    }
                  }
                }
              }
            }
          },
          "401": {
            "description": "Invalid credentials",
            "content": {
              "application/json": {
                "example": {
                  "status": "Failed",
                  "message": "Invalid email or password",
                  "isSuccess": false,
                  "data": null
                }
              }
            }
          }
        }
      }
    },
    "/users/register": {
      "post": {
        "tags": ["Users"],
        "summary": "Register new user",
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/RegisterRequest"
              }
            }
          }
        },
        "responses": {
          "201": {
            "description": "User registered successfully",
            "content": {
              "application/json": {
                "example": {
                  "status": "Success",
                  "message": "Registration successful",
                  "isSuccess": true,
                  "data": {
                    "id": 2,
                    "email": "zainal@gmail.com",
                    "name": "Zainal",
                    "role": "member"
                  }
                }
              }
            }
          },
          "400": {
            "description": "Invalid email format",
            "content": {
              "application/json": {
                "example": {
                  "status": "Failed",
                  "message": "Email format is invalid",
                  "isSuccess": false,
                  "data": null
                }
              }
            }
          }
        }
      }
    },
    "/users/me": {
      "get": {
        "tags": ["Users"],
        "summary": "Get current user information",
        "security": [
          {
            "bearerAuth": []
          }
        ],
        "responses": {
          "200": {
            "description": "User information retrieved successfully",
            "content": {
              "application/json": {
                "example": {
                  "status": "Success",
                  "message": "User retrieved successfully",
                  "isSuccess": true,
                  "data": {
                    "id": 1,
                    "email": "superadmin@binar.com",
                    "name": "Super Admin",
                    "role": "superadmin",
                    "createdAt": "2024-11-03T00:38:12.556Z",
                    "updatedAt": "2024-11-03T00:38:12.556Z",
                    "deletedAt": null
                  }
                }
              }
            }
          },
          "401": {
            "description": "Invalid token",
            "content": {
              "application/json": {
                "example": {
                  "status": "Failed",
                  "message": "Invalid token",
                  "isSuccess": false,
                  "data": null
                }
              }
            }
          }
        }
      }
    },
    "/users/admin": {
      "post": {
        "tags": ["Users"],
        "summary": "Create a new admin user (Superadmin only)",
        "security": [
          {
            "bearerAuth": []
          }
        ],
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "email": {
                    "type": "string",
                    "format": "email",
                    "example": "admin2@binar.com"
                  },
                  "password": {
                    "type": "string",
                    "example": "password123"
                  },
                  "name": {
                    "type": "string",
                    "example": "Admin 2"
                  }
                },
                "required": ["email", "password", "name"]
              }
            }
          }
        },
        "responses": {
          "201": {
            "description": "Admin created successfully",
            "content": {
              "application/json": {
                "example": {
                  "status": "Success",
                  "message": "Admin created successfully",
                  "isSuccess": true,
                  "data": {
                    "id": 3,
                    "email": "admin2@binar.com",
                    "name": "Admin 2",
                    "role": "admin"
                  }
                }
              }
            }
          },
          "403": {
            "description": "Access forbidden",
            "content": {
              "application/json": {
                "example": {
                  "status": "Failed",
                  "message": "Access forbidden",
                  "isSuccess": false,
                  "data": null
                }
              }
            }
          }
        }
      }
    },
    "/cars": {
      "post": {
        "tags": ["Cars"],
        "summary": "Create new car (Admin & SuperAdmin only)",
        "security": [
          {
            "bearerAuth": []
          }
        ],
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/CarRequest"
              }
            }
          }
        },
        "responses": {
          "201": {
            "description": "Car created successfully",
            "content": {
              "application/json": {
                "example": {
                  "status": "Success",
                  "message": "Car created successfully",
                  "isSuccess": true,
                  "data": {
                    "id": 1,
                    "name": "Toyota Camry",
                    "type": "Sedan",
                    "price": 35000,
                    "available": true,
                    "image": "https://example.com/car-image.jpg",
                    "createdBy": 1,
                    "updatedBy": 1,
                    "deletedAt": null,
                    "createdAt": "2024-11-03T03:06:11.807Z",
                    "updatedAt": "2024-11-03T03:06:11.807Z",
                    "creator": {
                      "id": 1,
                      "name": "Super Admin",
                      "email": "superadmin@binar.com"
                    }
                  }
                }
              }
            }
          },
          "403": {
            "description": "Access forbidden",
            "content": {
              "application/json": {
                "example": {
                  "status": "Failed",
                  "message": "Access forbidden",
                  "isSuccess": false,
                  "data": null
                }
              }
            }
          }
        }
      },
      "get": {
        "tags": ["Cars"],
        "summary": "Get all cars",
        "parameters": [
          {
            "in": "query",
            "name": "page",
            "schema": {
              "type": "integer",
              "default": 1
            },
            "description": "Page number"
          },
          {
            "in": "query",
            "name": "limit",
            "schema": {
              "type": "integer",
              "default": 10
            },
            "description": "Items per page"
          }
        ],
        "responses": {
          "200": {
            "description": "Cars retrieved successfully",
            "content": {
              "application/json": {
                "example": {
                  "status": "Success",
                  "message": "Cars retrieved successfully",
                  "isSuccess": true,
                  "data": {
                    "cars": [
                      {
                        "id": 1,
                        "name": "Toyota Camry",
                        "type": "Sedan",
                        "price": 35000,
                        "available": true,
                        "image": "https://example.com/car-image.jpg",
                        "createdBy": 1,
                        "updatedBy": 1,
                        "deletedAt": null,
                        "createdAt": "2024-11-03T03:06:11.807Z",
                        "updatedAt": "2024-11-03T03:06:11.807Z",
                        "creator": {
                          "id": 1,
                          "name": "Super Admin",
                          "email": "superadmin@binar.com"
                        }
                      }
                    ],
                    "pagination": {
                      "currentPage": 1,
                      "totalPages": 1,
                      "totalItems": 1,
                      "itemsPerPage": 10
                    }
                  }
                }
              }
            }
          },
          "500": {
            "description": "Internal server error",
            "content": {
              "application/json": {
                "example": {
                  "status": "Failed",
                  "message": "Failed to retrieve cars data",
                  "isSuccess": false,
                  "data": null
                }
              }
            }
          }
        }
      }
    }
  }
}
